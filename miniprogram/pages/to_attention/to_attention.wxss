/* pages/to_attention/to_attention.wxss */
.container {
    padding: 10rpx 30rpx;
}
.text_glass{
  background-color: rgba(255,255,255,0.35);
  border-top-left-radius: 25rpx 20rpx;
  border-top-right-radius: 25rpx 20rpx;
  border-bottom-left-radius: 25rpx 20rpx;
  border-bottom-right-radius: 25rpx 20rpx;
  box-shadow: rgba(50, 50, 93, 0.25) 0px 6px 12px -2px, rgba(50, 50, 93, 0.3) 0px 3px 7px -3px;
  padding: 15rpx 10rpx;
  margin-top: 15rpx;
  margin-bottom: 30rpx;
}
.div1 {
    display: flex;
    justify-content: center;
    margin-top: 10px;
}

.div1 image {
    width: 120rpx;
    height: 120rpx;
}
.div2{
    margin: 10rpx 10rpx;
}